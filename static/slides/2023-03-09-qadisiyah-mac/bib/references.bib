@article{Ertsen2011,
  title = {Robert {{Lawrence France}} (Ed): {{Wetlands}} of Mass Destruction. {{Ancient}} Presage for Contemporary Ecocide in Southern {{Iraq}}},
  author = {Ertsen, Maurits Willem},
  year = {2011},
  journal = {Water History},
  volume = {3},
