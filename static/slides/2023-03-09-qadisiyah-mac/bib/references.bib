@article{Soltysiak.Bialon2013,
  title = {Population History of the Middle {{Euphrates}} Valley: {{Dental}} Non-Metric Traits at {{Tell Ashara}}, {{Tell Masaikh}} and {{Jebel Mashtale}}, {{Syria}}},
  author = {So{\l}tysiak, Arkadiusz and Bialon, Marta},
  year = {2013},
  journal = {HOMO- Journal of Comparative Human Biology},
  doi = {10.1016/j.jchb.2013.04.005},
  abstract = {Fifty-nine dental non-metric traits were scored using Arizona State University Dental Anthropology System on a sample of teeth from 350 human skeletons excavated at three sites in the lower middle Euphrates valley. The dataset was divided into six chronological subsets: Early Bronze Age, Middle Bronze Age, Early Iron Age with Neo-Assyrian period, Classical/Late Antiquity, Early Islamic (Umayyad and Abbasid) period and Modern period. The matrix of Mean Measure of Divergence values exhibited temporal homogeneity of the sample with only dental non-metric trait scores in the Modern subset differing significantly from most other subsets. Such a result suggests that no major gene flow occurred in the middle Euphrates valley between the 3rd millennium BCE and the early 2nd millennium CE. Only after the Mongolian invasion and large depopulation of northern Mesopotamia in the 13th century CE a major population change occurred when the area was taken over in the 17th century by Bedouin tribes from the Arabian Peninsula. \textcopyright{} 2013 Elsevier GmbH.},
  groups = {Archaeology},
