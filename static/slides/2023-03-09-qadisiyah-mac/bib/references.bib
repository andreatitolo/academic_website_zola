@article{Marriner.etal2008,
  title = {Ancient {{Tyre}} and Its Harbours: 5000 Years of Human-Environment Interactions},
  shorttitle = {Ancient {{Tyre}} and Its Harbours},
  author = {Marriner, Nick and Morhange, Christophe and Carayon, Nicolas},
  year = {2008},
  month = may,
  journal = {Journal of Archaeological Science},
  volume = {35},
  number = {5},
  pages = {1281--1310},
  issn = {03054403},
  doi = {10.1016/j.jas.2007.09.003},
  abstract = {The exact location and evolution of Tyre's ancient harbour areas have been matters of scholarly conjecture since the 16th century. Here, we use geoscience techniques to precisely relocate the ancient northern harbour, the city's principal transport hub during antiquity, and reconstruct its geomorphological evolution (Marriner, N., Morhange, C., Boudagher-Fadel, M., Bourcier, M., Carbonel, P., 2005. Geoarchaeology of Tyre's ancient northern harbour, Phoenicia. Journal of Archaeological Science 32, 1302e1327.). While a natural anchorage is inferred during the Bronze Age, we expound the increasing weight of anthropogenic forcings from the Iron Age onwards, culminating in a technological apogee during the Byzantine period (Hohlfelder, R.L., 1997. Building harbours in the early Byzantine era: the persistence of Roman technology. Byzantinische Forschungen Internationale Zeitschrift fu\textasciidieresis r Byzantinistik 24, 367e389.). Using coastal stratigraphy and underwater archaeological data (El Amouri, M., El Helou, M., Marquet, M., Noureddine, I., Seco Alvarez, M., 2005. Mission d'expertise arche\textasciiacute ologique du port sud de Tyr, sud Liban: re\textasciiacute sultats preliminaries. Bulletin d'Arche\textasciiacute ologie et d'Architecture Libanaises, Hors-Se\textasciiacute rie 2, 91e110; Frost, H., Recent observations on the submerged harbour works at Tyre. Bulletin du Muse\textasciiacute e de Beyrouth 24, 103e111; Noureddine, I., Helou, M., 2005. Underwater archaeological survey in the northern harbour at Tyre. Bulletin d'Arche\textasciiacute ologie et d'Architecture Libanaises, Hors-Se\textasciiacute rie 2, 111e128) we demonstrate that Poidebard's (1939. Un grand port disparu, Tyr. Recherches ae\textasciiacute riennes et sous-marines, 1934e1936, Librairie Orientaliste Paul Geuthner, Paris) Egyptian harbour is in reality a drowned quarter of the ancient city. Informed hypotheses are proposed for a possible second anchorage at Tyre, and it is demonstrated that presently drowned portions of the sandstone ridge served as outer harbours during the Bronze and Iron Ages. Stratigraphic data in the vicinity of Tell Mashuk, Chawakir and Rachidiye are also presented and discussed.},
  langid = {english},
  groups = {Geoarchaeology},
  timestamp = {2022-02-03T10:10:40Z},
  file = {marriner2008a - Annotazioni Estratte (322022, 111219).md:/Users/andreatitolo/Obsidian/obsidian-personale/Z_Papers Annotation/marriner2008a - Annotazioni Estratte (322022, 111219).md:text/markdown;Marriner.etal2008.pdf:/Users/andreatitolo/Zotero/storage/KNNMREWL/Marriner.etal2008.pdf:application/pdf}
}

@article{Marriner.etal2012,
  title = {Ancient Harbors and {{Holocene}} Morphogenesis of the {{Ras Ibn Hani}} Peninsula ({{Syria}})},
  author = {Marriner, Nick and Goiran, Jean Philippe and Geyer, Bernard and Mato{\"i}an, Val{\'e}rie and {Al-Maqdissi}, Michel and Leconte, Marion and Carbonel, Pierre},
  year = {2012},
  journal = {Quaternary Research (United States)},
  volume = {78},
  number = {1},
  pages = {35--49},
  publisher = {{University of Washington}},
  doi = {10.1016/j.yqres.2012.03.005},
  abstract = {Ras Ibn Hani peninsula, a wave-dominated tombolo (800. \texttimes. 1000. m) on the Syrian coast, provides evidence for significant Holocene changes that can be linked to geological inheritance, rising post-glacial sea level, sediment supply and human impacts. Initial development of Ras Ibn Hani's coastal system began \textasciitilde. 8000. years ago when shallow marine environments formed in a context of rising post-glacial sea level. Following relative sea-level stabilization \textasciitilde. 6000. cal. yr BP, beach facies trace the gradual formation of a wave-dominated sandbank fronted by a \textasciitilde. 2300. \texttimes. \textasciitilde. 500. m palaeo-island whose environmental potentiality was attractive to Bronze Age societies. A particularly rapid phase of tombolo accretion is observed after \textasciitilde. 3500. cal. yr BP characterised by a two- to fourfold increase in sedimentation rates. This is consistent with (i) a pulse in sediment supply probably driven by Bronze Age/Iron Age soil erosion in local catchments, and (ii) positive feedback mechanisms linked to regionally attested neotectonics. Archaeological remains and radiocarbon datings confirm that the subaerial tombolo was probably in place by the Late Bronze Age. These data fit tightly with other eastern Mediterranean tombolo systems suggesting that there is a great deal of predictability to their geology and stratigraphy at the regional scale. \textcopyright{} 2012 University of Washington.},
  keywords = {Bronze Age,Coasts,Geoarchaeology,Geomorphology,Harbor,Holocene,Levant,Ras Ibn Hani,Syria,Tombolo,Ugarit},
  groups = {Geoarchaeology},
  timestamp = {2021-09-23T09:10:53Z},
  file = {Marriner.etal2012.pdf:/Users/andreatitolo/Zotero/storage/54L4E2KE/Marriner.etal2012.pdf:application/pdf}
}

@article{Marriner.Morhange2008,
