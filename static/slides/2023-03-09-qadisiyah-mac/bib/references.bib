@article{Colosi.etal2017,
  title = {Analysis and {{Processing}} of {{Nadir}} and {{Stereo VHR Pleiad\'es Images}} for {{3D Mapping}} and {{Planning}} the {{Land}} of {{Nineveh}}, {{Iraqi Kurdistan}}},
  author = {Colosi, Francesca and Malinverni, Eva and Pierdicca, Roberto and Orazi, Roberto and Bozzi, Carlo},
