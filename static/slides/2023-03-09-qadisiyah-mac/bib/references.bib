@article{Fowler2004,
  title = {Declassified {{CORONA KH-4B}} Satellite Photography of Remains from {{Rome}}'s Desert Frontier},
  author = {Fowler, M. J.F.},
  year = {2004},
  journal = {International Journal of Remote Sensing},
  volume = {25},
  number = {18},
  pages = {3549--3554},
  doi = {10.1080/0143116031000098887},
  abstract = {At its height under the Emperor Septimius Severus (AD 193\textendash 211), the Roman Empire covered over 3 millionkm2 and stretched from what is now central Scotland in the north-west to the Red Sea in the south-east (Scarre 1996). Whilst much is known about the military installations that secured the northern and western provinces of the Empire, the defences that secured the eastern and southern provinces are less well understood. In the case of the eastern desert frontier, there are many fine remains to be found and aerial photography conducted in the first half of the twentieth century has contributed much to our understanding (Kennedy and Riley 1990). However, since the Second World War, restrictions on civil aerial photography in this region have meant that systematic campaigns of aerial reconnaissance have not been possible until quite recently (Kennedy 2002). The declassification in 1995 of an archive of over 860 000 images acquired by the first generation of US photoreconnaissance satellites flown between 1960 and 1972 represents an important historical resource (McDonald 1995a). The archaeological potential of this resource was soon recognized, not the least because of its low cost and extensive coverage of `denied territories' in eastern Europe and Asia (e.g. Fowler 1996). As a result, CORONA imagery has been exploited in the prospection for archaeological features in a number of places including Turkey (Kennedy 1998), Syria (Philip et al. 2002) and Armenia (Palmer 2002). To illustrate the ability of the CORONA product to support archaeological studies, the Cover shows KH-4B imagery of the remains of five Roman military sites (listed in table 1) on the Empire's eastern desert frontier.},
  groups = {CORONA},
  timestamp = {2021-09-23T09:09:17Z},
  file = {Fowler2004.pdf:/Users/andreatitolo/Zotero/storage/53ALUAAD/Fowler2004.pdf:application/pdf}
}

