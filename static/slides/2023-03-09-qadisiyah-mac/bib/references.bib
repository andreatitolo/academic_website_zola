  shorttitle = {Mapping {{Archaeology While Mapping}} an {{Empire}}},
  author = {Petrie, Cameron A. and Orengo, Hector A. and Green, Adam S. and Walker, Joanna R. and Garcia, Arnau and Conesa, Francesc and Knox, J. Robert and Singh, Ravindra N.},
  year = {2019},
  month = jan,
  journal = {Geosciences},
  volume = {9},
  number = {1},
  pages = {11},
  publisher = {{Multidisciplinary Digital Publishing Institute}},
  issn = {2076-3263},
  doi = {10.3390/geosciences9010011},
  abstract = {A range of data sources are now used to support the process of archaeological prospection, including remote sensed imagery, spy satellite photographs and aerial photographs. This paper advocates the value and importance of a hitherto under-utilised historical mapping resource\textemdash the Survey of India 1'' to 1-mile map series, which was based on surveys started in the mid\textendash late nineteenth century, and published progressively from the early twentieth century AD. These maps present a systematic documentation of the topography of the British dominions in the South Asian Subcontinent. Incidentally, they also documented the locations, the height and area of thousands of elevated mounds that were visible in the landscape at the time that the surveys were carried out, but have typically since been either damaged or destroyed by the expansion of irrigation agriculture and urbanism. Subsequent reanalysis has revealed that many of these mounds were actually the remains of ancient settlements. The digitisation and analysis of these historic maps thus creates a unique opportunity for gaining insight into the landscape archaeology of South Asia. This paper reviews the context within which these historical maps were created, presents a method for georeferencing them, and reviews the symbology that was used to represent elevated mound features that have the potential to be archaeological sites. This paper should be read in conjunction with the paper by Arnau Garcia et al. in the same issue of Geosciences, which implements a research programme combining historical maps and a range of remote sensing approaches to reconstruct historical landscape dynamics in the Indus River Basin.},
  copyright = {http://creativecommons.org/licenses/by/3.0/},
  langid = {english},
  keywords = {archaeological landscapes,Archaeological Survey of India,colonial studies,heritage,historical maps,settlements,sovietmaps,Survey of India},
  groups = {Remote Sensing,SovietMaps},
  timestamp = {2022-05-18T17:25:41Z},
  file = {Petrie.etal2019.pdf:/Users/andreatitolo/Zotero/storage/6K7URXV8/Petrie.etal2019.pdf:application/pdf;Snapshot:/Users/andreatitolo/Zotero/storage/NUJE6MQW/11.html:text/html}
}

