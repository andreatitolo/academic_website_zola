  langid = {english},
  keywords = {A≈°≈°ur-dan II,Assyria's imperial phase,Assyrian kings,Assyrian power,internal fragmentation,Neo-Assyrian Period,Reconquista Period,Tiglath-pileser III},
  annotation = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/9781118325216.ch8},
  timestamp = {2022-09-27T09:38:42Z},
