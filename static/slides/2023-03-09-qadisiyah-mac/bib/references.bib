  publisher = {{Routledge}},
  doi = {10.1179/009346910X12707321520477},
  abstract = {The integration of spatial datasets from historical satellite imagery, digital elevation models (DEMs), and past archaeological surveys provides new insights into the nature and remains of past landscape transformations. Using southern Mesopotamia as a case study, this article addresses, both quantitatively and qualitatively, long-held assumptions concerning the nature and relationship of settlement patterns and river channel systems in antiquity. GIS and image analysis are used to fill in gaps in the settlement record and propose a revised location for the Tigris River during most of antiquity. Given that only one-third of the central alluvial plain had been ground surveyed in southern Mesopotamia, how complete was our picture of landscape and settlement? How could gaps in settlement be interpreted? The present work in the area east of Baghdad suggests that archaeologists and historians have underestimated the nature and movements of the Tigris River. Satellite imagery can help reveal the location of the Tigris River prior to its settling into its modern course, shedding light on its potential role in the rise of early Mesopotamian agricultural societies. The work presented here proposes a methodology for unweaving and mapping preserved pieces of ancient landscapes, addressing larger issues of human modification of the landscape. \textcopyright{} Trustees of Boston University 2010.},
  keywords = {gis,GIS,landscape,Landscape,remote sensing,Remote sensing,river channels,River channels,settlement patterns,Settlement patterns,southern mesopotamia,Southern Mesopotamia},
  groups = {Remote Sensing for Archaeology},
  timestamp = {2023-01-14T16:31:50Z},
  file = {Hritz2010a.pdf:/Users/andreatitolo/Zotero/storage/AKN95D3K/Hritz2010a.pdf:application/pdf}
}

@article{Hritz2014,
