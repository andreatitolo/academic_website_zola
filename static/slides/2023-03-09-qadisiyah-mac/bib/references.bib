@article{Moyes.etal2014,
  title = {Ancient {{Maya Regional Settlement}} and {{Inter-Site Analysis}}: {{The}} 2013 {{West-Central Belize LiDAR Survey}}},
  author = {Moyes, Holley and Iannone, Gyles and Brown, M. and Awe, Jaime and Chase, Diane and Fernandez Diaz, Juan and Carter, William and Chase, Arlen and Yaeger, Jason and Weishampel, John and Shrestha, Ramesh},
  year = {2014},
  journal = {Remote Sensing},
  volume = {6},
