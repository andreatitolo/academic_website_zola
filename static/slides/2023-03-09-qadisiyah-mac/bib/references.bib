@article{Braemer.etal2010,
  title = {Conquest of New Lands and Water Systems in the Western {{Fertile Crescent}} ({{Central}} and {{Southern Syria}})},
  author = {Braemer, Frank and Geyer, Bernard and Castel, Corinne and Abdulkarim, Maamoun},
  year = {2010},
  journal = {Water History},
  volume = {2},
  number = {2},
  pages = {91--114},
  doi = {10.1007/s12685-010-0029-9},
  abstract = {Apart from hydraulic systems linked to major perennial rivers, the control of water in the Near East passes by the exploitation of complementary seasonal resources: springs, water table, river floods, flash floods. It is particularly the case in the areas of contact between plains and mountains and in steppe transition zones. The water is managed through technical installations (springs capture, diversion dams, canals, cisterns, qanats) which need to be understood within historically contingent situations. The results of three French fields research programs covering the area spreading from the Aleppo plateau to the Yarmuk drainage basin shed new lights on development of water systems and the conquest of new agricultural and breeding areas during the third millennium, the roman\textendash byzantine times and the 19th century AD.},
  keywords = {19th century AD,Canal,Cistern,Diversion dam,Early Bronze Age,Qanat,Roman-byzantine period,Springs capture,Syria},
  groups = {Archaeology},
  timestamp = {2021-09-23T09:08:23Z},
  file = {Braemer.etal2010.pdf:/Users/andreatitolo/Zotero/storage/CAE6WBAA/Braemer.etal2010.pdf:application/pdf}
}

@article{Braemer2011,
  title = {Badia and {{Maamoura}}, the {{Jawlan}}/{{Hawran}} Regions during the {{Bronze Age}}: Landscapes and Hypothetical Territories},
  author = {Braemer, Frank},
  year = {2011},
  month = jan,
  journal = {Syria},
  number = {88},
  pages = {31--46},
  publisher = {{OpenEdition}},
  doi = {10.4000/syria.891},
  abstract = {Electronic reference Frank Braemer, \guillemotleft{} Badia and Maamoura, the Jawlan/Hawran regions during the Bronze Age: landscapes and hypothetical territories \guillemotright, Syria [Online] R\'esum\'e-30 ans apr\`es l'article de S. Helms "Land behind Damascus: Urbanism during the 4th Millenium in Syria/Palestine.", les derni\`eres prospections et fouilles en Syrie du Sud permettent de proposer une d\'efinition des diff\'erents terroirs du Jawlan et du Hawran basaltiques jusqu'au massif du Kalamun. Les particularit\'es physiques de chacun de ces terroirs induisent des syst\`emes adapt\'es de gestion de l'eau et d'organisation agricole et pastorale. Par ailleurs, une analyse de l'habitat et des \'echanges d'artefacts permet de caract\'eriser des \guillemotleft{} territoires arch\'eologiques \guillemotright{} ou \guillemotleft{} entit\'es culturelles \guillemotright. Les changements intervenant au cours des IV e et III e mill\'enaires sont mis en relief et un cadre \guillemotleft{} historique \guillemotright{} est propos\'e. Abstract-30 years after S. Helms's paper "Land behind Damascus: Urbanism during the 4th Millenium in Syria/ Palestine.", new fields works and surveys allow the definition of specific landscapes (from the basaltic Hawran and Jawlan to the Kalamun mountains) in which the physical setting induces particular systems of water management and agricultural or pastoral organization. On one another hand, an analysis of architectural techniques and forms, pottery exchanges, lithics, defines "archaeological territories" or "cultural entities". The topic of people mobility between territories is central. Changes which occurred during the Bronze Age are pointed out, and a historical frame for the 4 th and the 3 rd millennia proposed.},
  groups = {Landscape Archaeology},
  timestamp = {2021-09-23T09:08:23Z},
  file = {Braemer2011.pdf:/Users/andreatitolo/Zotero/storage/RF7MSEJH/Braemer2011.pdf:application/pdf}
}

